            <span class="hljs-keyword">for</span> (<span class="hljs-keyword">let</span> i = <span class="hljs-number">0</span>; i &lt; timeString.<span class="hljs-property">length</span>; i++) {
                <span class="hljs-keyword">const</span> span = clockSpans[i];
