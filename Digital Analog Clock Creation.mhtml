        <span class="hljs-keyword">function</span> <span class="hljs-title function_">updateClock</span>(<span class="hljs-params"></span>) {
            <span class="hljs-keyword">const</span> now = <span class="hljs-keyword">new</span> <span class="hljs-title class_">Date</span>();
